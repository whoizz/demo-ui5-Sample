{
  "name": "demo-ui5-sample",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "devDependencies": {
    "@sap/ui5-builder-webide-extension": "1.1.x",
    "@ui5/cli": "^2.0.3",
    "eslint": "^5.16.0",
    "sapui5-runtime": "^1.5.5",
    "ui5-middleware-servestatic": "^0.1.6",
    "ui5-task-nwabap-deployer": "^1.0.10",
